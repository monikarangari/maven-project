
pipeline
{
agent any
stages
{
  stage('scm checkout')
  {steps {git branch: 'master' , url: 'https://github.com/monikarangari/maven-project'}}
  
 stage('code build'){
steps{withMaven(globalMavenSettingsConfig: '3f8bb0dc-be09-46c7-9930-a5bf27ce791a', jdk: 'LocalJDK', maven: 'LocalMaven', mavenSettingsConfig: '--- Use system default settings or file path ---') 
{ sh 'mvn clean package' }
}

}
  }}
